<script type="text/javascript">
	apiready = function() {
		api.parseTapmode();
		id = api.pageParam[1].id;

		$cjy.ajax({
			url: index_url+'api/News/messageDetail',
			method: 'post',
			data:{'id':id}
		}, function(ret) {
			console.log(JSON.stringify(ret));
			if(ret.code == 0){
				$('.content').html(ret.data.content);
			}
			// toast1(ret.msg);
		});
		console.log(JSON.stringify(api.pageParam));
	};
</script>
