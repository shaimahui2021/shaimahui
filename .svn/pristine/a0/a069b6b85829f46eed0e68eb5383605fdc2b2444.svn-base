<script type="text/javascript">
	var currentPageNum1 =1;
	var currentPageNum2 =1;
	var currentPageNum3 =1;
	var listData1 = new Array();
	var listData2 = new Array();
	var listData3 = new Array();
	var replyData = {};
	var type = 1;//1.点赞 2.评论 3.系统消息
	var dialog = new auiDialog();
	apiready = function() {
		var userInfo = $api.getStorage('userInfo');
		var is_free = userInfo.is_free;
		if(is_free == 1){
			openDialog2('widget://app/dialog/dialog');
		}
		api.addEventListener({
				name: 'refreshIndex4'
			}, function(ret, err) {
				var userInfo = $api.getStorage('userInfo');
				var is_free = userInfo.is_free;
				if(is_free == 1){
					openDialog2('widget://app/dialog/dialog');
					return;
				}
				console.log("刷新了页面~~~~");
				window.location.reload();
		});
		var newestNum = $api.getStorage('newestNum');
		$("#notice").html("["+newestNum['notice']+"]");
		$("#comments").html("["+newestNum['comment']+"]");
		$("#like").html("["+newestNum['like']+"]");
		api.parseTapmode();
		var tab = new auiTab({
			element: document.getElementById("tab"),
		}, function(ret) {
			console.log(ret.index);
			if(ret.index==1){
				$('#praiseListData').html("");
				$('#praise').show();
				$('#comment').hide();
				$('#sysmsg').hide();
				$("#like").html("[0]");
				type = 1;
				currentPageNum1=1;
			}else if(ret.index==2){
				
				$('#commentListData').html("");
				$('#praise').hide();
				$('#comment').show();
				$('#sysmsg').hide();
				$("#like").html("[0]");
				$("#comments").html("[0]");
				type = 2;
				currentPageNum2=1;
			}else if(ret.index==3){
				$('#messageListData').html("");
				$('#praise').hide();
				$('#comment').hide();
				$('#sysmsg').show();
				type = 3;
				currentPageNum3=1;
				$("#notice").html("[0]");
				$("#like").html("[0]");
			}
			init(type);
		});

		api.addEventListener({
				name : 'scrolltobottom',
				extra : {threshold : 50 }//设置距离底部多少距离时触发，默认值为0，数字类型
		}, function(ret, err) {
				if(type == 1){
					currentPageNum1 += 1;
					getPraise(currentPageNum1);
				}else if(type == 2){
					currentPageNum2 += 1;
					getComment(currentPageNum2);
				}else if(type ==3){
					currentPageNum3 += 1;
					getNotice(currentPageNum3);
				}
		});

		api.setRefreshHeaderInfo({
		    loadingImg : 'widget://image/refresh.png',
		    bgColor : '#ccc',
		    textColor : '#fff',
		    textDown : '下拉刷新...',
		    textUp : '松开刷新...'
		}, function(ret, err) {
		    	currentPageNum1 = 1;
				currentPageNum2 = 1;
				currentPageNum3 = 1; 
				if(type == 1){
					getPraise(currentPageNum1);
				}else if(type == 2){
					getComment(currentPageNum2);
				}else{
					getNotice(currentPageNum3);
				}
		    // 恢复下拉
		    api.refreshHeaderLoadDone()
		});
		init(type);
		// openDialog('widget://app/dialog/dialog-reply');
	};

	function init(type){
		if(type == 1){
			getPraise(currentPageNum1);
		}else if(type == 2){
			getComment(currentPageNum2);
		}else{
			getNotice(currentPageNum3);
		}
	}

	//获取系统消息
	function getNotice(page){
		$cjy.ajax({
			url: index_url+'api/News/platformMessage',
			method: 'post',
			// showProgress: "努力加载中...",
			data:{
			'page':page
			}
		}, function(ret) {
			if(ret.code == 0){
				if(ret.data.length > 0){
					if(currentPageNum3==1){
						upMessageData(ret.data[0]['id'],"notice");
					}
					var data = {};
					data.listData = currentPageNum3 != 1 ? listData3.concat(ret.data) : ret.data;
					listData3 = data.listData;
					var html = template('Data-list-message',data);
					$api.html($api.byId('messageListData'), html);
				}else{
					if(currentPageNum3 == 1){
						$api.html($api.byId('messageListData'), '暂无数据~~~~');
					}else{
						currentPageNum3-=1;
					}
					
				}
			}
		},function(err){
		console.log(JSON.stringify(err));
		});
	}

	//获取点赞列表
	function getPraise(page){
		$cjy.ajax({
			url: index_url+'api/News/likeAll',
			method: 'post',
			data:{'page':page}
		}, function(ret) {
			 console.log(JSON.stringify(ret));
			if(ret.code == 0){
				if(ret.data.length > 0){
					if(currentPageNum1==1){
						upMessageData(ret.data[0]['id'],"like");
					}
					var data = {};
					data.listData = currentPageNum1 != 1 ? listData1.concat(ret.data) : ret.data;
					data.index_url = index_url;
					data.head_path = head_path;
					listData1 = data.listData;
					console.log($api.jsonToStr(data));
					var html = template('Data-list-praise',data);
					$api.html($api.byId('praiseListData'), html);
				}else{
						if(currentPageNum1 == 1){
							$api.html($api.byId('praiseListData'), '暂无数据~~~~');
						}else{
							currentPageNum1-=1;
							toast1('没有更多了！');
						}
				}
			}
		});
	}

	//获取评论列表
	function getComment(page){
		$cjy.ajax({
			url: index_url+'api/News/commentList',
			method: 'post',
			data:{'page':page}
		}, function(ret) {
			 console.log(JSON.stringify(ret.data));
			if(ret.code == 0){
				if(ret.data.length > 0){
					if(currentPageNum2==1){
						upMessageData(ret.data[0]['id'],"comment");
					}
					var data = {};
					data.listData = page != 1 ? listData2.concat(ret.data) : ret.data;
					data.index_url = index_url;
					data.head_path = head_path;
					data.nickname = $api.getStorage('userInfo').nickname;
					listData2 = data.listData;
					var html = template('Data-list-comment',data);
					$api.html($api.byId('commentListData'), html);
				}else{
					if(currentPageNum2 == 1){
						$api.html($api.byId('commentListData'), '暂无数据~~~~');
					}else{
						currentPageNum2-=1;
						toast1('没有更多了！');
					}
				}
			}
		});
	}

	//保存点击获得的信息
	function getReplyData(id,a_member_id,post_id){
		replyData = {
			'id':id,
			'a_member_id':a_member_id,
			'post_id':post_id,
			'member_id':$api.getStorage('userInfo').id
		}
	}

	function saveReplyData(content){
		replyData.content = content;
		// console.log(JSON.stringify(replyData));
		$cjy.ajax({
			url: index_url+'api/News/reply',
			method: 'post',
			data:replyData
		}, function(ret) {
			toast1(ret.msg);
			if(ret.code == 0){
				init(type);
			}
		});
	}
	
	function upMessageData(id,type){
		console.log("type==>"+type+",id===>"+id);
		//更新 数据
		var newestId = $api.getStorage('newestId');
		newestId[type] = id;
	 	$api.setStorage('newestId', newestId);
		api.writeFile({
			path: 'fs://33'+$api.getStorage('userId')+'.txt',
			data: JSON.stringify(newestId)
		}, function(ret, err) {
					
		}); 
	}
	
	
	
	
	
	


</script>
