<script type="text/template" id="addfocus-icon-list">
	<%if(isAttention == 0) {%>
	<i class="addfocus-icon" tapmode onclick="focusToggle(this,<%=id%>,<%=isAttention%>)"></i>
	<% }else {%>
	<i class="addfocus-icon active" tapmode onclick="focusToggle(this,<%=id%>,<%=isAttention%>)"></i>
	<% } %>
</script>
<script type="text/template" id="swiper-wrapper-list">
	<% for(var i = 0; i<Notice.length; i++){ %>
	<% if(Notice[i]['is_disable'] == 0) { %>
	<div class="swiper-slide" style="height: 7.5rem;">
		<% if(Notice[i]['url']) {%>
		<img src="<%=head_path + Notice[i]['head_path']%>" alt="" style="width: 100%;height: 7.5rem;" class="img-round-sm" tapmode onclick="openWin('widget://app/home/html/webbrowser_win','<%=Notice[i]['url']%>')">
		<% }else { %>
		<img src="<%=head_path + Notice[i]['head_path']%>" alt="" style="width: 100%;height: 7.5rem;" class="img-round-sm">
		<% } %>
	</div> 
	<% } %>
	<% } %>
</script>
