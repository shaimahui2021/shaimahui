	</div>
</script>
<script type="text/template" id="post-list">
	<% for(var i=0; i<post.length; i++) {%>
	<% if(post[i]['forward_id']){%>
	<ul class="aui-list aui-list-in aui-list-noborder aui-media-list">
		<li class="aui-list-item aui-list-item-middle aui-padded-15">
			<div class="aui-media-list-item-inner align-start">
				<div class="aui-list-item-media" style="width: 3rem;"  tapmode onclick="openWin('widget://app/derby/html/perscenter_win',{member_id:<%=post[i]['member_id']%>})">
					<img src="<%=head_path + post[i]['m_head_path']%>" class="aui-img-round aui-list-img-sm">
				</div>
				<div class="aui-list-item-inner">
					<div class="aui-list-item-text">
						<div class="aui-list-item-title flex-1 aui-font-size-16 font-bold aui-ellipsis"><%=post[i]['nickname']%></div>
					</div>
					<div class="aui-list-item-text">
						<div class="aui-font-size-12 aui-ellipsis"><%=post[i]['create_time']%></div> 
					</div>
					<div class="aui-list-item-text">
						<div class="aui-font-size-14">
						   <%=post[i]['forward']%> 
						</div>
					</div> 
				</div>
				<% if(thisMember == post[i]['member_id']) {%>
				<div class="aui-list-item-media aui-flex-col aui-flex-right aui-padded-r-0"> 
				    <!-- 删除关注 -->
					<i class="addfocus-icon delfocus-icon" tapmode onclick="delPost(<%=post[i]['id']%>)"></i>
				</div> 
				<% } %>
			</div>
			<div class="aui-media-list-item-inner align-start aui-padded-l-15 aui-padded-t-5 aui-padded-b-5 img-round-sm bg-gray"  tapmode onclick="openWin('widget://app/derby/html/mainbody_win',{post_id:<%=post[i]['forward_id']%>,member_id:<%=post[i]['member_id']%>})">
				<div class="aui-list-item-inner">
					<div class="aui-list-item-text">
						<div class="flex flex-row">
							<div class="flex-none color-blue aui-margin-r-5">@<%=post[i]['forward_nickname']%>:</div>
							<div class="aui-text-left"><%=post[i]['content']%></div>
						</div>
					</div>
				</div>
				
			</div>
		</li> 
	</ul>
	<% } else { %>
	<div class="aui-card-list aui-padded-t-10 aui-margin-b-0">
		<div class="aui-card-list-header">
		   <ul class="aui-list aui-list-in aui-media-list aui-list-noborder w-100">
			  <li class="aui-list-item aui-list-item-noactive aui-list-item-middle aui-padded-l-0">
				<div class="aui-media-list-item-inner">
			        <div class="aui-list-item-media" style="width: 3.5rem;">
				         <img src="<%=head_path + post[i]['m_head_path']%>" class="aui-img-round">
				    </div>
				    <div class="aui-list-item-inner aui-padded-r-0">
				    	<div class="aui-list-item-text">
				    		<div class="aui-list-item-title aui-font-size-16 aui-ellipsis"><%=post[i]['nickname']%> </div>
				    	</div>
				    	<div class="aui-list-item-text">
				    		<div class="aui-font-size-12 aui-ellipsis">
				    			<%=post[i]['create_time']%>
				    		</div>
				    	</div>
				    </div>
					<% if(thisMember == post[i]['member_id']) {%>
					<div class="aui-list-item-media aui-flex-col aui-flex-right aui-padded-r-0"> 
					    <!-- 删除关注 -->
						<i class="addfocus-icon delfocus-icon" tapmode onclick="delPost(<%=post[i]['id']%>)"></i>
					</div> 
					<% } %>
			    </div>
			  </li>
		   </ul> 
		</div>
		<div class="aui-card-list-content-padded aui-padded-t-5">
			<div class="aui-ellipsis-3">
				<span style="white-space: pre-line"><%=post[i]['content']%></span>
			</div>
			<% if(post[i]['head_path']) {%>
			<% if(post[i]['video_path'] != 0) {%>
			<div id="myVideoControls"  tapmode onclick="openVideo('<%=post[i]['video_path']%>');" class="videoList aui-padded-t-5">
			    <div class="videoListItem">
				    <% for(var j = 0; j<post[i]['head_path'].length; j++) {%>
				    <% if(post[i]['head_path'][j]!="未选择文件") {%>
				    <div id="uploaderFiles" class="videoListItemImg" data-src="<%=head_path + post[i]['head_path'][j]%>" style="background: url('<%=head_path + post[i]['head_path'][j]%>') no-repeat;background-size: cover;"></div>
				    <% } %>
				    <% } %>
				    <div id="videoBtn"></div>
				</div> 
			</div>
			<% } else { %>
			<div class="aui-row aui-row-padded aui-padded-t-5">
				<div class="aui-col-xs-12 uploadImgList">  
				     <% for(var j = 0; j<post[i]['head_path'].length; j++) {%> 
				        <% if(post[i]['head_path'][j]!="未选择文件") {%>
					         <div  <%if(post[i]['head_path'].length == 1) {%> class="picOnlyOne" <% } %> style="background: url(<%=head_path + post[i]['head_path'][j]%>) no-repeat;background-size: cover;" data-src="<%=head_path + post[i]['head_path'][j]%>"  tapmode onclick="openP(this,<%=j%>)"></div>
				        <% } %>
					<% } %>	
				</div> 
			</div>
			<% } %>
			<% } %>
		</div>
		<div class="aui-card-list-footer aui-padded-t-0">
			<div class="aui-info w-100">
				<% if(post[i]['isLike'] == 0) {%>
				<div class="aui-info-item"  id="dianzan<%=post[i]['id']%>"><img class="aui-margin-r-5 praise-icon" tapmode onclick="postLike(<%=post[i]['id']%>)" src="../../../image/praise.png" /><p id="p<%=post[i]['id']%>"> <%=post[i]['like']%></p></div>
				<% }else{ %>
				<div class="aui-info-item"  id="dianzan<%=post[i]['id']%>"><img class="aui-margin-r-5 praise-icon" tapmode onclick="postLike(<%=post[i]['id']%>)" src="../../../image/praise-active.png" /><p id="p<%=post[i]['id']%>"> <%=post[i]['like']%></p></div>
				<% } %>
				<div class="aui-info-item"  tapmode onclick="openWin('widget://app/derby/html/mainbody_win',{post_id:<%=post[i]['id']%>,member_id:<%=post[i]['member_id']%>})"><img class="aui-margin-r-5 msg-icon" src="../../../image/msg.png" /> <%=post[i]['comment_sum']%></div>
				<div class="aui-info-item"  tapmode onclick="openWin('widget://app/derby/html/forward_win',{post_id:<%=post[i]['id']%>,lottery_id:<%=post[i]['lottery_id']%>})"><img class="aui-margin-r-5 forward-icon" src="../../../image/forward.png" /> <p id="forwarding<%=post[i]['id']%>"><%=post[i]['forwarding']%></p></div>
				<div class="aui-info-item"  tapmode onclick="openDialog('widget://app/dialog/dialog-reward',{post_id:<%=post[i]['id']%>,type:3})"><img class="aui-margin-r-5 gift-icon" src="../../../image/gift-active.png" /> <%=post[i]['money']%></div>
			</div>
		</div>
	</div>
	<!-- 评论 -->
	<% if(post[i]['c_like'] >= 50){ %>
		<ul class="aui-list aui-list-in aui-media-list aui-list-noborder  border-t">
			<li class="aui-list-item aui-list-item-middle">
				<div class="aui-media-list-item-inner">
					<div class="aui-list-item-media" style="width: 3rem;"  tapmode onclick="openWin('widget://app/derby/html/perscenter_win',{member_id:<%=post[i]['c_member_id']%>})">
						<img src="<%=head_path + post[i]['c_head_path']%>" class="aui-img-round">
					</div>
					<div class="aui-list-item-inner">
						<div class="aui-list-item-text">
							<div class="aui-list-item-title aui-font-size-14 aui-ellipsis-1 nickName"><%=post[i]['c_nickname']%></div>
							<div class="aui-list-item-right">
								<div class="aui-info aui-padded-t-0 aui-padded-b-0">
									<div class="aui-info-item">
										<img class="aui-margin-r-15 godcomment-icon" src="../../../image/godcomment.png" />
										<% if(post[i]['c_isLike'] == 0) {%>
										<img  id="comment<%=post[i]['c_member_id']%>" class="aui-margin-r-10 praise-icon" tapmode onclick="commentLike(<%=post[i]['c_id']%>,<%=post[i]['c_member_id']%>)" src="../../../image/praise.png" />
										<% }else {%>
										<img  id="comment<%=post[i]['c_member_id']%>" class="aui-margin-r-10 praise-icon" tapmode onclick="commentLike(<%=post[i]['c_id']%>,<%=post[i]['c_member_id']%>)" src="../../../image/praise-active.png" />
										<% } %>
										<p id="conmentnum<%=post[i]['c_member_id']%>"><%=post[i]['c_like']%></p>
									</div>
								</div>
							</div>
						</div>
						<div class="aui-list-item-text aui-font-size-12">
							<%=post[i]['c_create_time']%>
						</div>
					</div>
				</div>
				<div class="aui-padded-r-15" style="padding-left: 3rem;">
					<div class="aui-ellipsis-1"><%=post[i]['c_content']%></div>
				</div>
			</li>
		</ul>
	<% } %> 
	<% } %>
	<div class="bg-gray" style="height: 0.5rem;"></div> 
	<% } %>
</script>
<script type="text/javascript">
	var member_id;
	var post_id;
	var lottery_id = 1;
	var dialog = new auiDialog();
	var videoScreenshots, videoPlayer, UIPhotoViewer, UILoading, UILoadingId;
	apiready = function() {
		api.parseTapmode();
		lotteryOpenType = api.pageParam[1];
		member_id = lotteryOpenType.member_id;
		post_id = lotteryOpenType.post_id;
		personal();
		videoPlayer = api.require('videoPlayer');
		videoScreenshots = api.require('videoScreenshots');
		UIPhotoViewer = api.require('UIPhotoViewer');
		UILoading = api.require('UILoading');
		//设置主题
		var skin = skinFunc('skin-red');
		var tab = new auiTab({
			element: document.getElementById("tab"),
		}, function(ret) {
			// alert(JSON.stringify(ret))
			api.sendEvent({
				name: 'removeSkinEvent'
			});
			if (ret.index == 1) {
				skin = skinFunc('skin-red');
				lottery_id = 1;
			} else if (ret.index == 2) {
				skin = skinFunc('skin-green');
				lottery_id = 2;
			} else if (ret.index == 3) {
				lottery_id = 3;
				skin = skinFunc('skin-blue');
			}
			personal();
			api.sendEvent({
				name: 'setSkinEvent'
			});
		});
		api.addEventListener({
			name: 'setSkinEvent'
		}, function(ret, err) {
			if (ret) {
				skin.setSkin();
			}
		});
		api.addEventListener({
			name: 'forwadrding'
		}, function(ret, err) {
			forwarding(ret.value.key1);
		});
		api.addEventListener({
			name: 'removeSkinEvent'
		}, function(ret, err) {
			if (ret) {
				skin.removeSkin();
			}
		});
	};

	function focusToggle(obj, member_id, is_attention) {
		$(obj).toggleClass('active');
		$cjy.ajax({
			url: configIndex.isAttention,
			method: 'post',
			data: {
				'a_member_id': member_id,
				'isAttention': is_attention
			}
		}, function(ret) {
			personal();
		});
	};
	//实例化主题
	function skinFunc(skinName) {
		return new auiSkin({
			name: skinName,
			skinPath: '../../../css/' + skinName + '.css',
			default: true
		});
	};

	function delPost(id) {
		dialog.alert({
			title: "提示",
			msg: '确认删除?',
			buttons: ['取消', '确定']
		}, function(ret) {
			if (ret.buttonIndex == 2) {
				$cjy.ajax({
					url: configIndex.delPost,
					method: 'post',
					data: {
						'record_id': id,
					}
				}, function(ret) {
					toast(ret.msg);
					if (post_id == id) {
						api.sendEvent({
							name: 'delpost',
						});
					}
					personal();
				});
			}
		})

	}

	function forwarding(post_id) {
		var num = $('#forwarding' + post_id).text();
		num = Number(num) + 1;
		$('#forwarding' + post_id).text(num);
		toast1('转发成功');
	}
	//帖子点赞
	function postLike(post_id) {
		changeImg(post_id);
		$cjy.ajax({
			url: configIndex.postLike,
			method: 'post',
			data: {
				'post_id': post_id,
				'a_member_id': member_id,
				'type': 1
			}
		}, function(ret) {});
	}
	//点赞改变样式
	function changeImg(post_id) {
		var path = $("#dianzan" + post_id).children().attr('src');
		if (path == '../../../image/praise.png') {
			var like = $("#p" + post_id).text();
			like = Number(like) + 1;
			$("#p" + post_id).text(like);
			path = '../../../image/praise-active.png'
		} else {
			var like = $("#p" + post_id).text();
			like = Number(like) - 1;
			$("#p" + post_id).text(like);
			path = '../../../image/praise.png'
		}
		$("#dianzan" + post_id).children().attr('src', path);

	};

	function commentLike(post_comment_id, post_member_id) {
		changeCommentImg(post_comment_id);
		$cjy.ajax({
			url: configIndex.postLike,
			method: 'post',
			data: {
				'post_comment_id': post_comment_id,
				'member_id': post_member_id,
				'type': 2
			}
		}, function(ret) {});
	};

	function changeCommentImg(id) {
		var path = $("#comment" + id).attr('src');
		if (path == '../../../image/praise.png') {
			var like = $("#conmentnum" + id).text();
			like = Number(like) + 1;
			$("#conmentnum" + id).text(like);
			path = '../../../image/praise-active.png'
		} else {
			var like = $("#conmentnum" + id).text();
			like = Number(like) - 1;
			$("#conmentnum" + id).text(like);
			path = '../../../image/praise.png'
		}
		$("#comment" + id).attr('src', path);

	};

	function personal() {
		$cjy.ajax({
			url: configIndex.personal,
			method: 'post',
			data: {
				'id': member_id,
				'lottery_id': lottery_id
			}
		}, function(ret) {
			var data = ret.data;
			data.member.head_path = head_path + data.member.head_path;
			data.head_path = head_path;
			$('#m-head-path').attr('src', data.member.head_path);
			$('#m-nickname').html(data.member.nickname);
			$('#m-autograph').html(data.member.autograph);
			var html1 = template('addfocus-icon-list', data.member);
			$api.html($api.byId('addfocus-icon'), html1);
			var html = template('perscenter-list', data.member);
			$api.html($api.byId('perscenter-tpl'), html);
			var html22 = template('swiper-wrapper-list', data);
			$api.html($api.byId('swiper-wrapper'), html22);
			swiperStart();
			var userInfo = $api.getStorage('userInfo');
			data.thisMember = userInfo.id;
			var text = template('post-list', data);
			$api.html($api.byId('post-tpl'), text);
			api.parseTapmode();
		}, function(err) {
			if (err.code === 103) {
				$("#post-tpl").empty();
			}
		});
	};

	function swiperStart() {
		var mySwiper = new Swiper('.swiper-container', {
			loop: true,
			autoplay: 3000,
			speed: 1000,
			pagination: '.swiper-pagination',
			prevButton: '.swiper-button-prev',
			paginationClickable: true,
			grabCursor: true,
			parallax: true,
			autoplayDisableOnInteraction: false,
		});
	};
	//视频未播放时提示加载中
	function openUILoading() {
		UILoading.keyFrame({
			rect: {
				w: api.winWidth / 2.0,
				h: api.winHeight / 2.0
			},
			styles: {
				bg: 'rgba(0,0,0,0.5)',
				corner: 5,
				interval: 50,
				frame: {
					w: 80,
					h: 80
				}
			}
		});
	};

	function closeUILoading() {
		UILoading.closeKeyFrame({
			isGradual: false
		});
	}
	//视频播放
	function openVideo(video_path) {
		video_image = $('#uploaderFiles').attr('data-src').replace('/', '');
		//打开一个frame用于专门播放短视频模块依附
		//去掉图片自动保存到手机相册
		// var currentTime = Math.round(new Date() / 1000);
		// var imgIP = '';
		// api.download({
			// url: video_image,
			// savePath: 'fs://' + currentTime + 'img.jpg',
			// report: true,
			// cache: true,
			// allowResume: true
		// }, function(ret, err) {
			// if (ret.state == 1) {
				// imgIP = ret['savePath'];
				// api.saveMediaToAlbum({
					// path: 'fs://' + currentTime + 'img.jpg'
				// }, function(ret, err) {
// 
				// });
				//打开一个frame用于专门播放短视频模块依附
				api.openFrame({
					name: 'video_fra',
					url: 'video_fra.html',
					rect: {
						x: 0,
						y: 0,
						w: 'auto',
						h: 'auto'
					},
				});
				openUILoading();
				videoPlayer.open({
					rect: {
						x: 0,
						y: 0,
						w: api.winWidth,
						h: api.winHeight
					},
					bg: '#000',
					// coverImg: 'fs://' + imgIP + '',
					path: video + video_path,
					fixedOn: 'video_fra',
					loop: true,
				}, function(ret, err) {
					if (ret.eventType == 'playing') {
						closeUILoading();
					}
					if (ret.status) {
						//监听单击播放器
						videoPlayer.addEventListener({
							name: 'click'
						}, function(ret, err) {
							if (ret) {
								videoPlayer.close();
								api.closeFrame({
									name: 'video_fra'
								});
							} else {
								toast('退出失败');
								api.closeFrame({
									name: 'video_fra'
								});
							}
						});
					} else {
						toast('播放失败');
						api.closeFrame({
							name: 'video_fra'
						});
					}
				});
			// } else {

			// }
		// });
	};
	// 关闭视频
	function closeVideo() {
		videoPlayer.close();
		api.closeFrame({
			name: 'video_fra'
		});
	};

	//预览图片
	function openP(obj, activeIndex) {
		var photoItem = [];
		var photoList = $(obj).parent().children();
		for (var i = 0; i < photoList.length; i++) {
			photoItem[i] = photoList.eq(i).attr("data-src");
		}
		UIPhotoViewer.open({
			images: photoItem,
			zoomEnabled: true,
			gestureClose: true,
			bgColor: '#000',
			activeIndex: activeIndex
		}, function(ret, err) {
			if (ret) {
				if (ret.eventType == 'change') {
					api.sendEvent({
						name: 'previewPage',
						extra: {
							key1: ret.index + 1,
							key2: photoList.length
						}
					});
				} else if (ret.eventType == 'click') {
					UIPhotoViewer.close();
					api.closeFrame({
						name: 'photoView2_fra'
					});
				} else if (ret.eventType == 'gestureColse') {
					api.sendEvent({
						name: 'gestureColse'
					});
				}
			}
		});
		setTimeout(function() {
			// var tabHeight = $api.offset($api.dom('#aui-header')).h; //tab切换栏高度
			api.openFrame({
				name: 'photoView2_fra',
				url: 'widget://app/derby/html/photoView2_fra.html',
				rect: {
					x: 0,
					y: 0,
					w: 'auto',
					h: 70
				},
				pageParam: {
					name: 'test'
				},
				bounces: false,
				bgColor: 'rgba(0,0,0,0)',
				vScrollBarEnabled: true,
				hScrollBarEnabled: true
			});
		}, 800);
	};
	//关闭预览
	function closeView() {
		UIPhotoViewer.close();
	};
</script>
